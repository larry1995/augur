#!/home/bears/augur-bear/virtualenvs/augur-bear/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'gitlab-issues-worker','console_scripts','gitlab_issues_worker_start'
__requires__ = 'gitlab-issues-worker'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('gitlab-issues-worker', 'console_scripts', 'gitlab_issues_worker_start')()
    )
